<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="sv.edu.udb.www.proyectodwf.entities.TipoEvento">
        <table name="tipo_evento" schema="dwf_proyecto" catalog=""/>
        <attributes>
            <id name="tipoEventoId">
                <column name="tipo_evento_id" precision="-1"/>
            </id>
            <basic name="nombreTipoEvento">
                <column name="nombre_tipo_evento" nullable="false" length="100"/>
            </basic>
            <basic name="descripcion">
                <column name="descripcion" nullable="false" length="200"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
